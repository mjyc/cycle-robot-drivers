{
  "private": true,
  "scripts": {
    "build": "mkdirp dist && browserify src/index.ts -p tsify --outfile dist/index.js -dv",
    "watch": "mkdirp dist && watchify src/index.ts -p tsify --outfile dist/index.js -dv",
    "start": "concurrently \"npm run watch\" \"live-server --watch=./dist/index.js,./index.html\""
  },
  "author": "Michael Jae-Yoon Chung",
  "license": "MIT",
  "dependencies": {
    "@cycle-robot-drivers/run": "^1.0.0",
    "xstream": "11.10.0"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "concurrently": "^4.0.1",
    "live-server": "^1.2.0",
    "mkdirp": "~0.5.0",
    "tsify": "^4.0.0",
    "typescript": "~3.2.0",
    "watchify": "^3.11.0"
  }
}
