    "@cycle/isolate": "5.0.0",
    "xstream": "11.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "concurrently": "^4.0.1",
    "live-server": "^1.2.0",
    "mkdirp": "~0.5.0",
    "watchify": "^3.11.0"
  }
